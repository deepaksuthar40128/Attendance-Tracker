

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" href="css/profHome.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    <div class="phone">
        <div class="topnav">
            
            <h3 class="text">Attendence <br> Traker</h3>
            <div class="ricon">
                <i class="fa fa-bars"></i>
            </div>
            <div class="licon">
                <i class="bi bi-person-fill"></i>
                <i class="bi bi-search"></i>
            </div>
            <% if( error !="" ){ %>
                <div class="error" role="alert">
                    <strong> Error </strong>
                    <%= error %>
                        <button type="button" onclick="document.getElementsByClassName('error')[0].remove()"
                            class="btn-close">x</button>
                </div>
                <% } %>
                    <% if( error_messages !="" ){ %>
                        <div class="error" role="alert">
                            <strong> Error </strong>
                            <%= error_messages %>
                                <button type="button" onclick="document.getElementsByClassName('error')[0].remove()"
                                    class="btn-close">x</button>
                        </div>
                        <% } %>
        </div>
      
        <hr>
        <div class="review">
            <h2>Review Attendence</h2>
        </div>
        
        <div class="subjects">
        </div>
    </div>

    <hr>

   
    <div class="actions">
        <h2 class="text">Actions</h2>
    </div>
    <div class="btns">
        <div class="prof" onclick="window.location.replace('/addCourse')"> Add Courses </div>
        <!-- <div class="prof"> Add Excel Test </div> -->
        <div class="prof"> Remove Courses </div>
    
    </div>

    <div class="foot">
        <i class="bi bi-house-door"></i>
        <i class="bi bi-journal-text"></i>
        <i class="bi bi-person"></i>
        <i class="bi bi-bell"></i>
    </div>
    </div>
    <script src="js/profHome.js"></script>
</body>

</html>